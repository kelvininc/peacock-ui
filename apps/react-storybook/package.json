{
	"name": "@kelvininc/react-storybook",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"clean": "pnpm clean:modules",
		"clean:build": "rm -rf storybook-static",
		"clean:modules": "rm -rf node_modules",
		"dev": "storybook dev -p 6006 --no-open",
		"prebuild": "pnpm clean:build",
		"build": "storybook build",
		"lint": "pnpm lint:check",
		"lint:check": "pnpm eslint:check",
		"lint:fix": "pnpm eslint:lint",
		"eslint:check": "eslint --quiet --cache",
		"eslint:lint": "eslint --quiet --cache --fix",
		"deploy": "storybook-to-ghpages --ci",
		"postinstall": "CUSTOM_INST=true node -e \"try{require('./node_modules/@kelvininc/react-ui-components/.scripts/copy-icons.js')}catch(e){}\""
	},
	"dependencies": {
		"@kelvininc/react-ui-components": "workspace:*",
		"@rjsf/core": "5.18.1",
		"@rjsf/utils": "5.18.1",
		"lodash-es": "^4.17.21",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.11.1",
		"@kelvininc/ui-components": "workspace:*",
		"@pxtrn/storybook-addon-docs-stencil": "^6.4.1",
		"@storybook/addon-actions": "^8.4.4",
		"@storybook/addon-docs": "^8.4.4",
		"@storybook/addon-essentials": "^8.4.4",
		"@storybook/addon-interactions": "^8.4.4",
		"@storybook/addon-links": "^8.4.4",
		"@storybook/addon-themes": "8.4.4",
		"@storybook/blocks": "^8.4.4",
		"@storybook/manager-api": "^8.4.4",
		"@storybook/preview-api": "^8.4.4",
		"@storybook/react": "^8.4.4",
		"@storybook/react-vite": "^8.4.4",
		"@storybook/storybook-deployer": "^2.8.16",
		"@storybook/theming": "^8.4.4",
		"@types/react": "^18.3.10",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.2",
		"eslint": "^9.11.1",
		"eslint-plugin-react-hooks": "^5.1.0-rc.0",
		"eslint-plugin-react-refresh": "^0.4.12",
		"eslint-plugin-storybook": "^0.9.0",
		"globals": "^15.9.0",
		"sass": "^1.53.0",
		"storybook": "^8.4.4",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.7.0",
		"vite": "^5.4.8"
	},
	"eslintConfig": {
		"extends": [
			"plugin:storybook/recommended"
		]
	}
}
